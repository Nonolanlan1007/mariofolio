{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src":"public/**/*",
      "use":"@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/(.+\\..+)",
      "dest": "/$1",
      "status": 200
    }
  ]
}